.dele-modal
  .ant-modal-content
    width 250px
    height 150px

    .ant-modal-body
      width 250px
      height 150px
      margin 0

      .ant-modal-confirm-body-wrapper
        width 100%
        height 100%
        display flex
        flex-direction column
        justify-content space-between

        .ant-modal-confirm-body
          .ant-modal-confirm-content
            margin-top 0
            font-size 17px

        .ant-modal-confirm-btns
          display flex
          justify-content flex-end

.class-table
  width 100%
  // height 100%
  height 840px
  padding 15px
  overflow hidden

  .tab-header
    display flex
    background #fff
    height 64px
    align-items center
    justify-content space-between
    padding-left 0px
    padding-right 30px
    margin-bottom 15px
    box-shadow -2px 3px 10px #d4d4d4

    .desc__wrap
      display flex
      align-items center
      // box-shadow 0 3px 10px #d4d4d4
      padding-left 15px
      border-left 4px solid #1890ff

      .anticon-left
        margin-right 15px

      .store
        height 32px
        padding 0 5px
        font-size 16px
        line-height 32px
        margin-right 30px
        color #777A88
        // box-shadow 0 3px 5px #d4d4d4

      .month
        height 32px
        padding 0 5px
        margin-right 5px
        font-size 16px
        line-height 32px
        color #777A88

      svg
        width 22px
        height 22px
        margin-right 10px
        margin-left 8px
        // border-left 5px solid #1890ff
        // box-shadow -2px 3px 5px #d4d4d4
    
    .save-button
      width 160px
      border-radius 5px
      background-color #1890ff
      color #fff

  .table-wrapper
    height 754px
    // box-shadow -2px 3px 10px #d4d4d4

    .week-header
      height 66px
      display flex
      background #fff
      width 100%
      margin-bottom 15px
      // background #f2f2f2
      background #fff
      // box-shadow -2px 3px 10px #d4d4d4
      color #777A88
      text-shadow #ebebeb 3px 3px 3px
      font-weight 550
      font-size 15px

      .ant-col-3:nth-child(1)
        background rgb(240, 242, 245)

      .week-header__title
        background #fff
        line-height 66px
        text-align center
        box-shadow 0 3px 5px #d4d4d4

      // .ant-col-3:nth-child(8)
      //   // border-right 1px solid #ebebeb
      //   line-height 66px
      //   text-align center
      //   box-shadow 0 3px 2px #d4d4d4

      // .ant-col-3
      //   border-right 1px solid #ebebeb
      //   line-height 66px
      //   text-align center

    .scroll-class::-webkit-scrollbar
      width 3px

    .scroll-class::-webkit-scrollbar-track
      border-radius 10px
      background #fff
      border-right 0.5px solid #EBEBEB

    .scroll-class::-webkit-scrollbar-thumb
      background-color rgba(0, 0, 0, 0.15)
      border-radius 8px
      width 4px

    .scroll-class::-webkit-scrollbar
      width 8px

    .scroll-class:hover
      overflow auto

    .scroll-class
      max-height 650px
      background #fff
      overflow hidden
      box-shadow -2px 3px 10px #d4d4d4

      .class-wrapper
        // min-height 226px
        background #fff
        border-bottom 1px solid #ebebeb
        display flex

        .class-type
          // border-right 1px solid #ebebeb
          text-align center
          font-size 16px
          color #777A88
          font-weight 550
          text-shadow #ebebeb 3px 3px 3px
          display flex
          justify-content space-around
          flex-direction column
          background #f2f2f2
          box-shadow -2px 2px 10px #d4d4d4
          // margin-top 15%
          // display table-cell
          // vertical-align middle
          // word-wrap break-word
          // letter-spacing 1000px
          // display flex
          // flex-direction column
          // justify-content flex-end
        
        .class-grid
          padding 15px 15px 22px 15px
          // min-height 226px
          border-left 1px solid #ebebeb
          box-shadow 0px 3px 0px 0px rgba(229,229,229,1)

          .single-class:hover .anticon
            display inline-block
            cursor pointer

          .single-class
            // border 1px solid #ebebeb
            border 1px solid #1890ff
            border-radius 10px
            // padding 10px
            margin-bottom 10px
            position relative

            .name-coach
              padding 8px
              display flex
              flex-direction column
              align-items start

              .single-class__name
                color #1890ff
                font-weight bold

              .single-class__coach
                color #777A88

            .time-room
              padding 8px
              // background orange
              background #1890ff
              display flex
              flex-direction column
              align-items start
              color #fff
              border-radius 0 0 10px 10px

              .single-class__time
                // border-top 1px solid #ebebeb
                color #fff
                border-radius 10px
                padding 0 5px
                background #0068b7

              .single-class__room
                padding 0 5px
                margin-bottom 3px

            .anticon:hover svg
              color #0068b7
              width 17px
              height 17px

            .anticon
              display none
              position absolute
              top 2px
              right 2px

          .add-class
            width 100%
            text-align center
            padding 8px
            border 2px solid #1890ff
            // border 1px transparent solid
            border-radius 10px
            // background #1890ff
            color #1890ff
            cursor pointer
            position relative
            outline none

          .add-class:hover
            transform scale(1.08)
